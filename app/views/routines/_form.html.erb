<%= form_with(model: @routine, local: true) do |form| %>

  <%= form.label :start_time, "開始時刻" %>
  <%= form.time_field :start_time %>
  
    <%= form.label :end_time, "終了時刻" %>
  <%= form.time_field :end_time %>
  
  <%= form.label :content, "内容" %>
  <%= form.text_field :content %>

  <%= form.submit yield(:button_text),  data: { "disable-with": yield(:button_text) } , class: 'btn' %>
<% end %>